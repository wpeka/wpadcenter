{
    "env": {
        "browser": true,
        "es2021": true
    },


        "extends": [ "plugin:@wordpress/eslint-plugin/react" ]
    ,
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "plugins": [
    ],
    "ignorePatterns": [ 
        "**/admin/js/gutenberg-blocks/*.js", 
        "**/admin/js/vue/*.js",
        "webpack.config.js",
        "webpack.config.gutenbergscripts.js",
        "jest.config.js",
        "jest-setup.js",
        "Gruntfile.js",
        "analytics/",
        "**/src/JSTests/*.js",
        "node_modules/"
    ],
    "rules": {
        "eqeqeq": "off",
        "camelcase": "off",
        "vars-on-top": "off",
        "no-shadow": "off",
        "no-alert": "off",
        "linebreak-style": "off",
        "no-unused-vars": "off",
        "no-mixed-operators": "off",
        "no-redeclare": "off",
        "no-unused-expressions": "off",
        "no-bitwise": "off",
        "indent": "off",
        "no-nested-ternary":"off",
        "array-callback-return":"off"
    }
    
}
